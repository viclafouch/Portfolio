{
  "version": 2,
  "name": "portfolio",
  "builds": [
    {
      "use": "@now/static"
    }
  ],
  "alias": ["www.victor-de-la-fouchardiere.fr"],
  "env": {},
  "public": true,
  "github": {
    "enabled": false
  },
  "routes": [
    {
      "src": "/_next/static/(chunks|runtime|commons)/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/static/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
