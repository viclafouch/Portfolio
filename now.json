{
  "version": 2,
  "builds": [{ "src": "./out/**", "use": "@now/static" }],
  "env": {},
  "routes": [
    { "src": "/formations", "dest": "out/formations.html" },
    { "src": "/404", "dest": "out/404.html" },
    { "src": "/competences", "dest": "out/competences.html" },
    { "src": "/index", "dest": "out/index.html" },
    { "src": "/productions", "dest": "out/productions.html" },
    { "src": "/blog", "dest": "out/blog.html" },
    { "src": "/(.*)", "dest": "out/$1" },
    {
      "src": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/_next/static/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/static/.*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
